{% extends 'base.html.twig' %}

{% block title %}Chambre classique | HotelHouse
{% endblock %}

{% block body %}

	<h1 class="text-center my-4">
		Chambre classique</h1>

	<div class="row">
		<div class="col-8 mx-auto">
			{{form_start(form)}}

            {{form_row(form.date_debut)}}
            {{form_row(form.date_fin)}}
            {{form_row(form.prenom)}}
            {{form_row(form.nom)}}
            {{form_row(form.telephone)}}
            {{form_row(form.email)}}


			{{form_end(form)}}
			</div>
		</div>

	{% endblock %}
